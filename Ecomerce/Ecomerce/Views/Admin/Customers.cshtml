@model List<Customer>
@{
    ViewData["Title"] = "Customers";
}
<link href="~/css/ownerpage.css" rel="stylesheet" />

<h1 style="text-align:center">Customers</h1>

<div class="container" style="min-height:600px;">

    

    <partial name="_SideNavBar" />
    <div style="flex-basis:15%"></div>

    <table class="table table-striped table-dark">

        <thead>
            <tr>
                <th>
                    Id
                </th>
                <th>
                    FName
                </th>
                <th>
                    LName
                </th>
                <th>
                    Email
                </th>
                <th>
                    Address
                </th>
                <th>
                    Phone
                </th>
                <th>
                   More
                </th>

            </tr>
        </thead>
        <tbody>

            @{
                for (int i = 0; i < Model?.Count(); i++)
                {
                    <tr>
                        <td>
                            @Model[i].CustomerId
                        </td>
                        <td>
                            @Model[i].FirstName
                        </td>
                        <td>
                            @Model[i].LastName
                        </td>
                        <td>
                            @Model[i].Email
                        </td>
                        <td>
                            @Model[i].Address
                        </td>

                        <td>
                            @Model[i].PhoneNumber
                        </td>
                        <td>
                            <a>More</a>
                        </td>
                    </tr>




                }
            }




        </tbody>



    </table>




</div>
